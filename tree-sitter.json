{
  "grammars": [
    {
      "name": "rvparam",
      "camelcase": "RVParam",
      "scope": "source.rvparam",
      "path": ".",
      "file-types": ["cpp", "hpp", "cfg", "rvmat"],
      "injection-regex": "^rvparam$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "RVParam grammar for treesitter",
    "authors": [
      {
        "name": "simonvic",
        "email": "simonvic.dev@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/simonvic/tree-sitter-rvparam"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false
  }
}
